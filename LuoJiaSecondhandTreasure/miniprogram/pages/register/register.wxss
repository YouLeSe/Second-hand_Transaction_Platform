/* pages/register/register.wxss */
.register{
  border-radius: 25px;
  border: lightgray solid ;
  box-shadow: lightgray 10rpx 10rpx 10rpx;
  margin-top: 12%;
  margin-left: 8%;
  margin-right: 8%;
  /* background-color: violet; */
  justify-content: center;
  display: flex;
  flex-direction: column;
}
.whuicon{
  padding: 40rpx;
  display: flex;
  justify-content: center;
}
.whuicon image{
  width: 200rpx;
  height: 200rpx;
}
.titleArea{
  width: 100%;
  /* background-color: white; */
  display: flex;
  justify-content: center;
  margin-bottom: 20rpx;
}
.title{
  font-size: 40rpx;
}
.registerImf{
  align-self: center;
  width: 90%;
  margin-top: 20px;
  /* background-color: teal; */
  margin-bottom: 40rpx;
}
.registerButton{
  align-self: center;
  width: 70%;
}
.registers{
  font-size: 20rpx;
  align-self: center;
  margin-bottom: 40rpx;

}