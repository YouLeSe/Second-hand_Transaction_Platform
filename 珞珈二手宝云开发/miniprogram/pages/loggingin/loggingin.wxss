/* pages/logingin/logingin.wxss */
.loggingin{
  border-radius: 25px;
  border: lightgray solid ;
  box-shadow: lightgray 10rpx 10rpx 10rpx;
  margin-top: 35%;
  margin-left: 8%;
  margin-right: 8%;
  /* background-color: violet; */
  justify-content: center;
  display: flex;
  flex-direction: column;
}
.whuicon{
  padding: 40rpx;
  display: flex;
  justify-content: center;
}
.whuicon image{
  width: 200rpx;
  height: 200rpx;
}
.titleArea{
  width: 100%;
  /* background-color: white; */
  display: flex;
  justify-content: center;
  margin-bottom: 20rpx;
}
.title{
  font-size: 40rpx;
}
.logginginImf{
  align-self: center;
  width: 100%;
  /* background-color: teal; */
  margin-bottom: 40rpx;
}
.logginginButton{
  align-self: center;
  width: 80%;
  margin-bottom: 10rpx;

}
.register{
  font-size: 20rpx;
  align-self: center;
  margin-bottom: 50rpx;
}